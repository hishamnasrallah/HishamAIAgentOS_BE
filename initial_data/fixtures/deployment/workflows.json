  {
    "model": "workflows.workflow",
    "pk": "c29810c9-a15a-485c-8f37-fe0fd1817d7f",
    "fields": {
      "name": "Feature Development",
      "slug": "feature-development",
      "description": "Complete feature development lifecycle from story to production",
      "definition": {
        "name": "Feature Development",
        "version": "1.0",
        "description": "Complete feature development lifecycle from story to production",
        "metadata": {
          "author": "HishamOS",
          "created_date": "2024-12-01",
          "tags": [
            "feature",
            "development",
            "agile"
          ]
        },
        "steps": [
          {
            "id": "create_story",
            "name": "Generate User Story",
            "agent": "Business Analyst Agent",
            "inputs": {
              "feature_idea": "{{input.feature_description}}",
              "product_context": "{{input.product_vision}}"
            },
            "on_success": "design",
            "max_retries": 2
          },
          {
            "id": "design",
            "name": "Technical Design",
            "agent": "Research Analyst",
            "inputs": {
              "user_story": "{{steps.create_story.output}}",
              "system_architecture": "{{input.architecture}}"
            },
            "on_success": "implement",
            "max_retries": 2
          },
          {
            "id": "implement",
            "name": "Implement Feature",
            "agent": "Research Analyst",
            "inputs": {
              "technical_design": "{{steps.design.output}}",
              "user_story": "{{steps.create_story.output}}"
            },
            "on_success": "review",
            "on_failure": "design",
            "max_retries": 3,
            "timeout_seconds": 900
          },
          {
            "id": "review",
            "name": "Code Review",
            "agent": "Research Analyst",
            "inputs": {
              "code_changes": "{{steps.implement.output}}",
