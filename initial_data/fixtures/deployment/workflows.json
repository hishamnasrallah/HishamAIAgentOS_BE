  {
    "model": "workflows.workflow",
    "pk": "3cb6a6c4-692b-4ab8-8332-84fea41e0e29",
    "fields": {
      "name": "Bug Lifecycle",
      "slug": "bug-lifecycle",
      "description": "Complete bug lifecycle from report to deployment and closure",
      "definition": {
        "name": "Bug Lifecycle",
        "version": "1.0",
        "description": "Complete bug lifecycle from report to deployment and closure",
        "metadata": {
          "author": "HishamOS",
          "created_date": "2024-12-01",
          "tags": [
            "bug",
            "sdlc",
            "qa",
            "development"
          ]
        },
        "steps": [
          {
            "id": "triage",
            "name": "Bug Triage",
            "agent": "Research Analyst",
            "inputs": {
              "bug_description": "{{input.bug_description}}",
              "project_context": "{{input.project}}"
            },
            "on_success": "assign",
            "on_failure": "notify_team",
            "max_retries": 2
          },
          {
            "id": "assign",
            "name": "Assign to Developer",
            "agent": "Research Analyst",
            "inputs": {
              "bug_info": "{{steps.triage.output}}",
              "team_capacity": "{{input.team_capacity}}"
            },
            "condition": "{{steps.triage.output.severity}} > 2",
            "on_success": "fix",
            "max_retries": 1
          },
          {
            "id": "fix",
            "name": "Implement Fix",
            "agent": "Research Analyst",
            "inputs": {
              "bug_details": "{{steps.triage.output}}",
              "assigned_dev": "{{steps.assign.output.developer}}"
            },
            "on_success": "code_review",
            "on_failure": "escalate",
            "max_retries": 3,
            "timeout_seconds": 600
          },
          {
            "id": "code_review",
            "name": "Code Review",
            "agent": "Research Analyst",
            "inputs": {
              "code_changes": "{{steps.fix.output.code}}",
              "bug_context": "{{steps.triage.output}}"
            },
            "on_success": "qa_test",
            "on_failure": "fix",
            "max_retries": 2
          },
          {
            "id": "qa_test",
            "name": "QA Testing",
            "agent": "Research Analyst",
            "inputs": {
              "fix_description": "{{steps.fix.output}}",
              "test_scenarios": "{{steps.triage.output.test_cases}}"
            },
            "on_success": "deploy",
            "on_failure": "fix",
            "max_retries": 2
          },
          {
            "id": "deploy",
            "name": "Deploy Fix",
            "agent": "Research Analyst",
            "inputs": {
              "deployment_package": "{{steps.qa_test.output}}",
              "environment": "production"
            },
            "on_success": "close",
            "on_failure": "rollback",
            "max_retries": 1,
            "timeout_seconds": 900
          },
          {
            "id": "close",
            "name": "Close Bug",
            "agent": "Research Analyst",
            "inputs": {
              "bug_id": "{{input.bug_id}}",
              "resolution": "{{steps.deploy.output}}"
            },
            "max_retries": 1
          },
          {
            "id": "escalate",
            "name": "Escalate to Senior Dev",
            "agent": "Research Analyst",
            "inputs": {
              "bug_info": "{{steps.triage.output}}",
              "failure_reason": "{{steps.fix.error}}"
            },
            "on_success": "fix"
          },
          {
            "id": "rollback",
            "name": "Rollback Deployment",
            "agent": "Research Analyst",
            "inputs": {
              "deployment_id": "{{steps.deploy.output.deployment_id}}"
            },
            "on_failure": "notify_team"
          },
          {
            "id": "notify_team",
            "name": "Notify Team of Failure",
            "agent": "Research Analyst",
            "inputs": {
              "workflow_state": "{{steps}}",
              "error_details": "Critical workflow failure"
            }
          }
        ]
      },
      "version": "1.0",
      "status": "active",
      "is_template": false,
      "execution_count": 0,
      "created_by": null,
      "created_at": "2025-12-06T07:48:23.278Z",
      "updated_at": "2025-12-06T07:48:23.278Z"
    }
  },
  {
    "model": "workflows.workflow",
    "pk": "52a5a50f-6f0c-40ee-8c19-49bceb34d92e",
    "fields": {
      "name": "Code Review",
      "slug": "code-review",
      "description": "Comprehensive code review with automated checks and peer review",
      "definition": {
        "name": "Code Review",
        "version": "1.0",
        "description": "Comprehensive code review with automated checks and peer review",
        "metadata": {
          "author": "HishamOS",
          "created_date": "2024-12-01",
          "tags": [
            "code-review",
            "quality",
            "collaboration"
          ]
        },
        "steps": [
          {
            "id": "automated_checks",
            "name": "Run Automated Checks",
            "agent": "Research Analyst",
            "inputs": {
              "code_diff": "{{input.code_changes}}",
              "repo_url": "{{input.repository}}"
            },
            "on_success": "peer_review",
            "on_failure": "request_fixes",
            "max_retries": 1
          },
          {
            "id": "peer_review",
            "name": "Peer Code Review",
            "agent": "Research Analyst",
            "inputs": {
              "code_changes": "{{input.code_changes}}",
              "automated_report": "{{steps.automated_checks.output}}",
              "review_guidelines": "{{input.guidelines}}"
            },
            "condition": "{{steps.automated_checks.output.passed}} == true",
            "on_success": "approval_check",
            "on_failure": "request_fixes",
            "max_retries": 1,
            "timeout_seconds": 600
          },
          {
            "id": "approval_check",
            "name": "Check Approval Status",
            "agent": "Research Analyst",
            "inputs": {
              "review_feedback": "{{steps.peer_review.output}}"
            },
            "condition": "{{steps.peer_review.output.issues_count}} == 0",
            "on_success": "approve",
            "on_failure": "request_fixes"
          },
          {
            "id": "request_fixes",
            "name": "Request Code Fixes",
            "agent": "Research Analyst",
            "inputs": {
              "review_comments": "{{steps.peer_review.output.comments}}",
              "author": "{{input.author}}"
            },
            "skip_if": "{{steps.approval_check.output.approved}} == true"
          },
          {
            "id": "approve",
            "name": "Approve Changes",
            "agent": "Research Analyst",
            "inputs": {
              "pull_request": "{{input.pr_id}}",
              "reviews": "{{steps}}"
            },
            "max_retries": 1
          }
        ]
      },
      "version": "1.0",
      "status": "active",
      "is_template": false,
      "execution_count": 1,
      "created_by": null,
      "created_at": "2025-12-06T07:48:23.291Z",
      "updated_at": "2025-12-07T14:10:28.307Z"
    }
  },
  {
    "model": "workflows.workflow",
